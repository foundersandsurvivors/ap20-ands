    src_text.number := 0;
    src_text.string := txt;
    IF txt SIMILAR TO E'¬ß\\d+%' THEN
        src_text.number := (REGEXP_MATCHES(txt, E'^¬ß(\\d+)'))[1]::INTEGER;
        src_text.string := REGEXP_REPLACE(txt, E'^¬ß\\d+ ', ''); -- strip ¬ßn from text
    END IF;
    RETURN src_text;
END
$_$;


