CREATE FUNCTION _append(text, text, text) RETURNS text
    LANGUAGE sql STABLE
    AS $_$
-- private string concatenation function
-- used with get_place_name() and get_person_name() below
SELECT
    CASE WHEN $2 <> '' AND $2 NOT LIKE '-%'
    THEN
        $1 || $3 || $2
    ELSE
        $1
    END
$_$;


